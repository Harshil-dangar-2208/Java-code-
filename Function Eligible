import java.util.*;

public class Function_eligible {
  public static Boolean solution(int age){
    if(age > 18){
      return true;
    }else{
      return false;
    }
  }
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int age = sc.nextInt();
      
      System.out.println(solution(age));
  }
}
