import java.util.*;

public class Functions {
  public static int Multiplication(int a,int b){
    return a*b;
  }
    public static void main(String[] args) {
      
      Scanner Sc = new Scanner (System.in);
      int a  = Sc.nextInt();
      int b  = Sc.nextInt();


      System.out.println("Multiplication of 2 numbers = "+Multiply(a,b));
  }
  
}
