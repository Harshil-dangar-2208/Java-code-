import java.util.*;

public class Functions {
  public static void Factorial(int n){
    
    int Factorial = 1;
    
    for(int i=n; i>=1; i--){
      Factorial = Factorial * i;
      
    }
    
    System.out.println(Factorial);
    return;
  }
    public static void main(String[] args) {
      
      Scanner Sc = new Scanner (System.in);
      int n  = Sc.nextInt();

Factorial(n);
  }
  
}
