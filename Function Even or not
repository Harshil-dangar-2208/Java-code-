import java.util.*;

public class function {
  public static void EvenORNot(int n){
    
    if(n % 2 == 0){
      System.out.println("Even numer");
    }
    else {
      System.out.println("Odd number");
    }
    return;
  }
    public static void main(String[] args) {
      
      Scanner sc = new Scanner(System.in);
      int n = sc.nextInt();
      
      EvenORNot(n);
  }
}
